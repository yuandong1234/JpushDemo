<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_register_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="获取注册id" />

        <Button
            android:id="@+id/btn_set_alias"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="设置Alias" />

        <Button
            android:id="@+id/btn_delete_alias"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="删除Alias" />

        <Button
            android:id="@+id/btn_set_tag"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="设置tag" />

        <Button
            android:id="@+id/btn_delete_tag"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="删除tag" />

        <Button
            android:id="@+id/btn_clear_tag"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="清空tag" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>